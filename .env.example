# Trading System Environment Variables
# Copy this file to .env and fill in your actual credentials
# NEVER commit the .env file to version control!

# Brokerage API Credentials
# Required for live trading with real brokerages (Public, Moomoo, etc.)
BROKERAGE_API_KEY=your_api_key_here
BROKERAGE_API_SECRET=your_api_secret_here

# Optional: Brokerage Account ID
# BROKERAGE_ACCOUNT_ID=your_account_id_here

# Data Provider API Keys
# Required only if using Alpha Vantage as data provider
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key_here

# Optional: Custom Configuration
# LOG_LEVEL=INFO
# CACHE_DIR=./data/cache
